<h2>Add Photo</h2>

<%= error_messages_for :images %>

<% form_for(:image, :url => { :action => "create" }, :html => { :multipart => true }) do |f| -%>

<p>Upload Photo:<br/> <%= f.file_field :uploaded_data %></p>

<p>
<%= submit_tag "Add Photo" %>
 or 
<%= button_to_function "Go Back", "window.location='/image/'" %>
</p>

<% end -%>
